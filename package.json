{
  "name": "alternate-file",
  "displayName": "Alternate File",
  "description": "Find and create spec files for any framework",
  "version": "0.2.6",
  "publisher": "will-wow",
  "author": "Will Ockelmann-Wagner",
  "repository": {
    "type": "git",
    "url": "https://github.com/alternate-file/alternate-file.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.11.3"
  },
  "keywords": [
    "alternate",
    "spec",
    "test"
  ],
  "bin": {
    "alt": "./bin/cli.js"
  },
  "icon": "assets/icon.png",
  "main": "dist/alternate-file.cjs.js",
  "module": "dist/alternate-file.js",
  "types": "dist/index.d.ts",
  "files": [
    "src/**/*",
    "dist/**/*",
    "bin/**/*",
    "sample-projections/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "preversion": "npm run lint && npm run test",
    "test": "jest",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.ts'",
    "format:prettier": "prettier --write '**/*.{ts,json,md}'",
    "format:types": "tsc --noEmit",
    "lint": "npm-run-all -c lint:*",
    "lint:eslint": "eslint '**/*.ts'",
    "lint:prettier": "prettier --check '**/*.{ts,json,md}'",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "codecov": "^3.8.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "tmp": "^0.2.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/node": "^17.0.23",
    "find-up": "^5.0.0",
    "minimist": "^1.2.6",
    "pipeout": "^0.1.0",
    "result-async": "^1.3.0"
  }
}
